#!/bin/bash

char=z
for i in {1..5}; do
	if [ $i = 1 ]; then
		char=a
	elif [ $i = 2 ]; then
		char=b
	elif [ $i = 3 ]; then
		char=c
	elif [ $i = 4 ]; then
		char=d
	elif [ $i = 5 ]; then
		char=f
	fi
	mv run${char}_50M_5M_1 tmp.txt
	sed -n '1,1250000p' tmp.txt > run${char}_50M_5M_1
	sed -n '1250001,2500000p' tmp.txt > run${char}_50M_5M_2
	sed -n '2500001,3750000p' tmp.txt > run${char}_50M_5M_3
	sed -n '3750001,5000000p' tmp.txt > run${char}_50M_5M_4
	rm tmp.txt
done

char=z
for i in {1..2}; do
        if [ $i = 1 ]; then
                char=a
        elif [ $i = 2 ]; then
                char=e
        fi
        mv load${char}_50M_1 tmp.txt
        sed -n '1,12500000p' tmp.txt > load${char}_50M_1
        sed -n '12500001,25000000p' tmp.txt > load${char}_50M_2
        sed -n '25000001,37500000p' tmp.txt > load${char}_50M_3
        sed -n '37500001,50000000p' tmp.txt > load${char}_50M_4
        rm tmp.txt
done

char=e
mv run${char}_50M_25M_1 tmp.txt
sed -n '1,625000p' tmp.txt > run${char}_50M_25M_1
sed -n '625001,1250000p' tmp.txt > run${char}_50M_25M_2
sed -n '1250001,1875000p' tmp.txt > run${char}_50M_25M_3
sed -n '1875001,2500000p' tmp.txt > run${char}_50M_25M_4
rm tmp.txt
